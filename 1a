package main

import (
	"fmt"
)

func main() {
	var num1, num2 int

	fmt.Println("Digite o primeiro número:")
	fmt.Scanln(&num1)

	fmt.Println("Digite o segundo número:")
	fmt.Scanln(&num2)

	var maior int

	if num1 > num2 {
		maior = num1
	} else {
		maior = num2
	}

	fmt.Println("O maior número é:", maior)
}
